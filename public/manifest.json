{
  "manifest_version": 3,
  "name": "Undistracted Me",
  "version": "3.0.0",
  "author": "Saroj Belbase",
  "description": "An extension that replaces your boring new tab pages",
  "icons": {
    "16": "favicon/lotus16.png",
    "32": "favicon/lotus32.png",
    "48": "favicon/lotus48.png",
    "128": "favicon/lotus128.png",
    "512": "favicon/lotus512.png"
  },
  "action": {
    "default_popup": "src/popup/index.html",
    "default_icon": {
      "16": "favicon/lotus16.png",
      "24": "favicon/lotus24.png",
      "32": "favicon/lotus32.png",
      "128": "favicon/lotus128.png"
    },
    "default_title": "Undistracted Me"
  },
  "chrome_url_overrides": {
    "newtab": "public/index.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{1071d037-0f17-4c92-b06c-704050d2e2c3}"
    }
  },
  "homepage_url": "https://whatsthemiti.sarojbelbase.com.np",
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval' http://localhost:5173; object-src 'self'"
  }
}